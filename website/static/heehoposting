server {
	server_name heehoposting.xyz www.heehoposting.xyz;

	location / {
		include uwsgi_params;
		uwsgi_pass unix:/home/jacks/heehoposting.xyz/heehoposting.sock;
	}

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/heehoposting.xyz/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/heehoposting.xyz/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.heehoposting.xyz) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = heehoposting.xyz) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


	listen 80;
	server_name heehoposting.xyz www.heehoposting.xyz;
    return 404; # managed by Certbot




}